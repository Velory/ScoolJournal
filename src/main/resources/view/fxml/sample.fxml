<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
            prefHeight="480.0" prefWidth="640.0" styleClass="background" stylesheets="@../css/theme.css"
            xmlns="http://javafx.com/javafx/8" fx:controller="sirotkina.sjournal.controller.LoginController">
    <children>
        <Label fx:id="lblEmail" layoutX="183.0" layoutY="129.0" prefHeight="21.0" prefWidth="80.0" text="Email"
               textFill="#215001"/>
        <TextField fx:id="userEmail" layoutX="183.0" layoutY="150.0" prefHeight="31.0" prefWidth="274.0">
            <opaqueInsets>
                <Insets/>
            </opaqueInsets>
        </TextField>
        <Label fx:id="lblPassword" layoutX="183.0" layoutY="198.0" prefHeight="21.0" prefWidth="80.0" text="Password"
               textFill="#224f02"/>
        <Button fx:id="login" defaultButton="true" layoutX="253.0" layoutY="287.0" mnemonicParsing="false"
                onAction="#onLogin" prefHeight="31.0" prefWidth="135.0" styleClass="background" stylesheets="@../css/theme.css"
                text="Login" textFill="WHITE"/>
        <Label fx:id="auth" alignment="CENTER" layoutX="183.0" layoutY="259.0" onMouseClicked="#onClickAuthorization"
               onMouseExited="#onExitedAuthorization" onMouseMoved="#onMovedAuthorization" prefHeight="21.0"
               prefWidth="274.0" style="-fx-background-color: #ffffff;" text="Authorization" textFill="#fa3242"/>
        <PasswordField fx:id="userPassword" layoutX="183.0" layoutY="219.0" prefHeight="31.0" prefWidth="274.0"/>
    </children>
</AnchorPane>
